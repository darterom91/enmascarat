<div class="modal fade bgcolor" id="modal_carrier_installed" name="modal_carrier_installed" tabindex="-1" role="dialog" aria-labelledby="modal_carrier_installed" aria-hidden="true">
        <div class="modal-dialog" style="width:70%">                
            <div class="modal-content">
                <div class="modal-header">                                
                    <h4 class="modal-title" id="myModalLabel">{l mod='genei' s ='Carrier installed'}</h4>
                </div>                        
                <div class="modal-body" style="width:40%;max-width:80%;">
                    {l mod='genei' s = 'Genei carrier installed succesfully'}
                </div>
                <div class="modal-footer">                                                        
                <a href="#"><button type="button" onclick="location.reload()" name="buttoncloseinstalledcarrier" id="buttoncloseinstalledcarrier" class="btn btn-default" data-dismiss="modal">{l mod='genei' s ='Close'}</button></a>                
                </div>
            </div>

        </div>
    </div>